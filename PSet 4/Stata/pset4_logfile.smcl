{com}. /*
> Title:          02_q2e.do
> Purpose:        Question 2.e, PSet 4
> 
> */
. /*
> 2.E Alter the cutoff.
> 
> */
. 
. use "$dta_loc/pset4_trim2.dta", clear
{txt}
{com}. 
. gen y = logwage 
{txt}
{com}. 
. local c_range 30(10)70
{txt}
{com}. 
. forval i = 1/41 {c -(}
{txt}  2{com}.         local c = 29+`i' // c= 30(1)70
{txt}  3{com}.         
.         gen margin_`c' = x - `c'/100
{txt}  4{com}.         gen win_`c'    = x > `c'/100
{txt}  5{com}. {c )-}
{txt}
{com}. 
. // Local linear regression
. /*
> forval i = 1/41 {c -(}
>         local c = 29+`i' // c= 30(1)70
>         if      `c' <  50 reg y i.win_`c'##c.margin_`c' if x < 0.5 
>         else if `c' == 50 reg y i.win_`c'##c.margin_`c'
>         else if `c' >  50 reg y i.win_`c'##c.margin_`c' if x > 0.5 
> {c )-}
> */
. 
. local rdopts ""
{txt}
{com}. forval i = 1/41 {c -(}
{txt}  2{com}.         
.         // get c
.         local c = 29+`i' // c= 30(1)70
{txt}  3{com}.         local c_reg = `c'/100
{txt}  4{com}.         
.         if      `c' <  50 rdrobust y x if x < 0.5, c(`c_reg') p(1) h(50/100) kernel(uniform)
{txt}  5{com}.         else if `c' == 50 rdrobust y x,                     c(`c_reg') p(1) h(50/100) kernel(uniform)
{txt}  6{com}.         else if `c' >  50 rdrobust y x if x > 0.5, c(`c_reg') p(1) h(50/100) kernel(uniform)
{txt}  7{com}.         
.         // get stats
.         local tau_`c' = `e(tau_cl)'
{txt}  8{com}.         dis "tau_`c' = `tau_`c''"
{txt}  9{com}.         local z_bc = e(tau_bc) / e(se_tau_rb)
{txt} 10{com}.         local bc_lb_`c' = e(tau_bc) - invnormal(0.975)*e(se_tau_rb)
{txt} 11{com}.         local bc_ub_`c' = e(tau_bc) + invnormal(0.975)*e(se_tau_rb)
{txt} 12{com}. {c )-}
{res}
